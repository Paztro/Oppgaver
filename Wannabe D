package oppgaver;

import java.util.Random;
import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);

		System.out.print("Oppgi arrayens lengde: ");
		int array = input.nextInt();

		System.out.println("Arrayen inneholder: ");

		int[] list = fyll(array);
		print(list);
		sjekk(list);

		for (int i = 0; i < list.length; i++) {

		}

	}

	// Returns array of given size composed of consecutive odds
	public static int[] fyll(int x) {  // riktig
		int[] data = new int[x];  // riktig
		for (int i = 0; i < data.length; i++) { 	// i < x; i++
			Random number = new Random();		// riktig
			int randnumb = number.nextInt(10) + 1; // Skal være random tall. fra det brukeren skrive inn. 
							       // Skriver personen inn 10, så skal det være 10 tall i størrelse 0-10
							       // data[i] = rand.nextInt(tall + 1);
			data[i] = i;		// Hva gjør denne?
			System.out.print(randnumb + " "); // Ikke noe print på på A
		}
		return data;
	}

	// adds one to each array element
	public static void print(int[] data) {		// riktig
		for (int i = 0; i < data.length; i++) {  	// Her skal du bruke forkorta version av for loop.
								// forelesning 10, side 35
			data[i]++;		// Print her
		}

	}

	public static void sjekk(int[] teller) {
		Scanner input1 = new Scanner(System.in);
		System.out.println();
		System.out.println();

		for (int i = 0; i < 3; i++) {
			System.out.print("Oppgi et tall (0 - 10): ");  // Hvorfor dette på sjekk? Dette skal ikke inn før D
			int number = input1.nextInt();

		}

	}
}
